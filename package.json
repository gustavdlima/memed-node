{
  "name": "memed-node",
  "version": "0.0.1",
  "description": "Cliente Node.js não-oficial para a API da Memed",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run test:run",
    "example": "tsx examples/basic.ts"
  },
  "keywords": [
    "memed",
    "node",
    "api",
    "healthcare",
    "prescription",
    "prescrição",
    "saúde"
  ],
  "author": "Gustavo Martins <gustavdlima@gmai.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gustavdlima/memed-node"
  },
  "bugs": {
    "url": "https://github.com/gustavdlima/memed-node/issues"
  },
  "homepage": "https://github.com/gustavdlima/memed-node#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18",
    "vitest-fetch-mock": "^0.4.5"
  }
}
